        {
          "dataset": "__internal_schema",
          "editorMode": "code",
          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "SELECT\n  MIN(latency_sec) AS min_latency_seconds,\n  MAX(latency_sec) AS max_latency_seconds,\n  AVG(latency_sec) AS avg_latency_seconds,\n  PERCENTILE_APPROX(latency_sec, 0.5) AS p50_latency_seconds,\n  PERCENTILE_APPROX(latency_sec, 0.95) AS p95_latency_seconds,\n  PERCENTILE_APPROX(latency_sec, 0.99) AS p99_latency_seconds\nFROM (\n  SELECT\n    TIMESTAMPDIFF(SECOND, time, ingestion_time) AS latency_sec\n  FROM web_server_logs\n  WHERE time IS NOT NULL AND ingestion_time IS NOT NULL\n  ORDER BY ingestion_time ASC\n  LIMIT 1000000 OFFSET 2\n) AS filtered;",

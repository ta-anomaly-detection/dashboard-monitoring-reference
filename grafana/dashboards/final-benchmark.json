      "targets": [
        {
          "dataset": "web_monitoring",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  CAST(DATE_FORMAT(ingestion_time, '%Y-%m-%d %H:%i:00') AS DATETIME) AS time_bucket,\n  MIN(TIMESTAMPDIFF(SECOND, time, ingestion_time)) AS min_latency_seconds,\n  MAX(TIMESTAMPDIFF(SECOND, time, ingestion_time)) AS max_latency_seconds,\n  AVG(TIMESTAMPDIFF(SECOND, time, ingestion_time)) AS avg_latency_seconds,\n  PERCENTILE_APPROX(TIMESTAMPDIFF(SECOND, time, ingestion_time), 0.5) AS p50_latency_seconds,\n  PERCENTILE_APPROX(TIMESTAMPDIFF(SECOND, time, ingestion_time), 0.95) AS p95_latency_seconds,\n  PERCENTILE_APPROX(TIMESTAMPDIFF(SECOND, time, ingestion_time), 0.99) AS p99_latency_seconds,\n  COUNT(*) AS event_count\nFROM web_server_logs\nWHERE ingestion_time >= NOW() - INTERVAL 1 HOUR\n  AND time IS NOT NULL AND ingestion_time IS NOT NULL\nGROUP BY time_bucket\nORDER BY time_bucket;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "web_server_logs"
